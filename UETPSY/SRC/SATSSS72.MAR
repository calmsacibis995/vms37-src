TEST_MOD_NAME_D: STRING	I,<SATSSS72>	; TEST MODULE NAME DESCRIPTOR
MSG1_INP_CTL:	STRING	I,<  SSCVA!4ZW: CONDITIONS:>
					; FAO CTL STRING FOR MSG1 IN SUCCOMMON.MAR
MSG3_ERR_CTL::	STRING	I,< *SSCVA!4ZW: !AS> 
					; FAO CTL STRING FOR MSG3 IN SUCCOMMON.MAR
PAGESIZE:	.LONG	512		; PAGE SIZE IN BYTES
	.PAGE
	.PSECT	RWDATA,RD,WRT,NOEXE,LONG
PRIVMASK:	.BLKQ	1		; ADDR OF PRIVILEGE MASK (IN PHD)
PAGCNT_ERG:	.BLKL	1		; PAGCNT ARGUMENT FOR EXPREG
RETADR_ERG:	.BLKQ	1		; RETADR ARGUMENT FOR EXPREG
