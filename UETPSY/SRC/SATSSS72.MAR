	COND	4,NOTARG,<LOCATION OF CREATED PAGES>,-
		  <END OF PROGRAM REGION>,-
		  <END OF CONTROL REGION>,-
		  <MIDDLE OF PROGRAM REGION>,-
		  <MIDDLE OF CONTROL REGION>,-
 
		    .LONG	0,1,2,3
;
	COND	5,NOTARG,<PAGE COUNT>,-
		  <ONE PAGE>,-
		  <SMALL COUNT>,-
		  <LARGE COUNT>,-
 
		    .LONG	1
		    .LONG	5
		    .LONG	25
