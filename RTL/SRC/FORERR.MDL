;+
; Define short valued symbols of the form FOR$K_symbol which are used in calls to
; the FORTRAN error Procedure of the form:

;	FOR$$SIGNAL (FOR$K_error_number) 
  
; which are changed into 32-bit VAX-11 condition values
; of the form FOR$_error_number 
; before calling LIB$SIGNAL (FOR$_error_number) 

; In macro, the symbols may be defined either as .EXTRNs or $FORERR
; 	$FORERR
; will define all FORTRAN error numbers.

;*****************************************************
;
; MAINTENANCE NOTE:
;
;	COM$$ERRSET_TST module (in file COMSETST.B32) contains the F4P/11
;	 compatibility error handler and has a table for all errors.
;	 When new error numbers are added that table may need modification.
;
;*****************************************************
;- 
  
  
  
	$STRUCT FOR,ERR		; define suffix ERR, not DEF

				; The following messages may not be the actual text:
				; See RTLMSG.MDL for the correct text
	K FAC_NO, 24		; FORTRAN facility number (STS$W_FAC_NO)

	K NOTFORSPE,	1	; NOT A FORTRAN-SPECIFIC ERROR
				; Used as a catch-all error number
				; for error other than FOR$ and MTH$
	K MINLUNERR,	17	; Minimum FORTRAN error which has a LUB/ISB/RAB
	K SYNERRNAM,	17	; SYNTAX ERROR IN NAMELIST INPUT
	K TOOMANVAL,	18	; TOO MANY VALUES IN NAMELIST INPUT
	K INVREFVAR,	19	; INVALID REFERENCE TO VARIABLE IN NAMELIST
	K REWERR,	20	; REWIND ERROR
	K DUPFILSPE,	21	; DUPLICATE FILE SPECIFICATIONS
	K INPRECTOO,	22	; INPUT RECORD TOO LONG
	K BACERR,	23	; BACKSPACE ERROR
	K ENDDURREA,	24	; END-OF-FILE DURING READ
	K RECNUMOUT,	25	; RECORD NUMBER OUTSIDE RANGE
	K OPEDEFREQ,	26	; OPEN OR DEFINEFILE REQUIRED TO SPECIFY DIRECT ACCESS
	K TOOMANREC,	27	; TOO MANY RECORDS IN I/O STATEMENT
	K CLOERR,	28	; CLOSE ERROR
	K FILNOTFOU,	29	; FILE NOT FOUND
	K OPEFAI,	30	; OPEN FAILURE
	K MIXFILACC,	31	; MIXED FILE ACCESS MODES
	K INVLOGUNI,	32	; INVALID LOGICAL UNIT NUMBER
	K ENDFILERR,	33	; ENDFILE ERROR
	K UNIALROPE,	34	; UNIT ALREADY OPEN
	K SEGRECFOR,	35	; SEGMENTED RECORD FORMAT ERROR
	K ATTACCNON,	36	; ATTEMPT TO ACCESS NON-EXISTENT RECORD
	K INCRECLEN,	37	; INCONSISTENT RECORD LENGTH
	K ERRDURWRI,	38	; ERROR DURING WRITE
	K ERRDURREA,	39	; ERROR DURING READ
	K RECIO_OPE,	40	; RECURSIVE I/O OPERATION
	K INSVIRMEM,	41	; INSUFFICIENT VIRTUAL MEMORY
	K NO_SUCDEV,	42	; NO SUCH DEVICE 
	K FILNAMSPE,	43	; FILE NAME SPECIFICATION ERROR 
	K INCRECTYP,	44	; INCONSISTENT RECORD TYPE
	K KEYVALERR,	45	; KEYWORD VALUE ERROR IN OPEN STATEMENT
	K INCOPECLO,	46	; INCONSISTENT OPEN/CLOSE PARAMETERS
	K WRIREAFIL,	47	; WRITE TO READONLY FILE
	K INVARGFOR,	48	; INVALID ARGUMENT TO FORTRAN RUN-TIME LIBRARY
	K INVKEYSPE,	49	; INVALID KEY SPECIFICATION
	K INCKEYCHG,	50	; INCONSISTENT KEY CHANGE OR DUPLICATE KEY
	K INCFILORG,	51	; INCONSISTENT FILE ORGANIZATION
	K SPERECLOC,	52	; SPECIFIED RECORD LOCKED
	K NO_CURREC,	53	; NO CURRENT RECORD
	K REWRITERR,	54	; REWRITE ERROR
	K DELERR,	55	; DELETE ERROR
	K UNLERR,	56	; UNLOCK ERROR
	K FINERR,	57	; FIND ERROR
	K LISIO_SYN,	59	; LIST-DIRECTED I/O SYNTAX ERROR
	K INFFORLOO,	60	; INFINITE FORMAT LOOP
	K FORVARMIS,	61	; FORMAT/VARIABLE-TYPE MISMATCH
	K SYNERRFOR,	62	; SYNTAX ERROR IN FORMAT
	K OUTCONERR,	63	; OUTPUT CONVERSION ERROR
	K INPCONERR,	64	; INPUT CONVERSION ERROR

	K OUTSTAOVE,	66	; OUTPUT STATEMENT OVERFLOWED RECORD
	K INPSTAREQ,	67	; INPUT STATEMENT REQUIRED TOO MUCH DATA
	K VFEVALERR,	68	; VARIABLE FORMAT EXPRESSION VALUE ERROR

	K MAXLUNERR,	69	; Maximum FORTRAN error code which has a LUB/ISB/RAB

;+
; Define arithmetic trap error numbers, even though signaled as SS$_ARITH
; Therefore there is no corresponding error message in FORMSG.MDL for these:
;-

	K INTOVF,	70	; INTEGER OVERFLOW
	K INTZERDIV,	71	; INTEGER ZERO DIVIDE
	K FLOOVE,	72	; FLOATING OVERFLOW
	K FLOZERDIV,	73	; FLOATING ZERO DIVIDE
	K FLOUND,	74	; FLOATING UNDERFLOW

	K DECSTROVE,	76	; DECIMAL STRING OVERFLOW
	K ARRREFOUT,	77	; ARRAY REFERENCE OUTSIDE ARRAY

; End of arithmetic trap error codes



	K ADJARRDIM,	93	; ADJUSTABLE ARRAY DIMENSION ERROR

	K MAX_ERR,	93	; maximum error number (NOT AN ERROR!!!)
	E			; end of FORTRAN error codes
; End of file FORERR.MDL
