MODULE RUNHAK (MAIN = START) =
!	The only purpose of RUNHAK is to act as a main program
!	that calls RUNOFF proper.  RUNHAK is needed only if
!	RUNOFF does not have a standard command line interface.
BEGIN
ROUTINE START : NOVALUE =
BEGIN
EXTERNAL ROUTINE RUNOFF;
RUNOFF ();
END;
END
ELUDOM
