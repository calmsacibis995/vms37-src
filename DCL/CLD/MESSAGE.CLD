define verb message
	image message
	prefix cli$k_mess_
	parameter p1,prompt="File",value(required,list,impcat,type=$infile)
	qualifier file,		value(type=$infile)
	qualifier listing,	value(type=$outfile),batch
	qualifier mdl,		value(type=$outfile)
	qualifier sdl,		value(type=$outfile)
	qualifier object,	value(type=$outfile),default
	qualifier symbols,	default
	qualifier text,		default
	outputs(object,listing,mdl,sdl)
