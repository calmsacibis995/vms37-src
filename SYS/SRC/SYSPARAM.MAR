;
	PARAMETER	ADDRESS=SCH$GL_AWSTIME,-	;
			DEFAULT=20,-	;
			MIN=1,-		;
			NAME=AWSTIME,-	;
			SIZE=LONG,-	;
			TYPE=<SYS,DYNAMIC>,-;
			UNIT=10Ms	;
;
; Swap rate control.  This parameter sets the swapping rate and serves to limit
