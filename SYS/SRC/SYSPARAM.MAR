	PARAMETER	ADDRESS=SCH$GL_WSINC,-	;
			DEFAULT=150,-	;
			MIN=0,-	;
			NAME=WSINC,-	;
			SIZE=LONG,-	;
			TYPE=<SYS,DYNAMIC,MAJOR>,-	;
			UNIT=Pages	;
