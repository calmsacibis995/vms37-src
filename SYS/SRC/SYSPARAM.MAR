	PARAMETER	ADDRESS=SCH$GW_AWSMIN,-	;
			DEFAULT=50,-	;
			MIN=0,-		;
			NAME=AWSMIN,-	;
			SIZE=WORD,-	;
			TYPE=<SYS,DYNAMIC>,-;
			UNIT=Pages	;
