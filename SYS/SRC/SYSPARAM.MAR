; outswap.
;
	PARAMETER	ADDRESS=SWP$GL_SWPPGCNT,-	;
			DEFAULT=60,-	;
			MIN=0,-	;
			NAME=SWPOUTPGCNT,-	;
			SIZE=LONG,-	;
			TYPE=<SYS,DYNAMIC>,-	;
			UNIT=Pages	;
