	PARAMETER	ADDRESS=MPW$GL_THRESH,-	;
			DEFAULT=200,-		;
			MIN=0,-			;
			MAX=16384,-		;
			NAME=MPW_THRESH,-	;
			SIZE=LONG,-		;
			TYPE=<SYS,DYNAMIC>	;
