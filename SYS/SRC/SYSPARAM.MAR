;
	PARAMETER	ADDRESS=SCH$GL_SWPRATE,-	;
			DEFAULT=500,-	;
			MIN=0,-	;
			NAME=SWPRATE,-	;
			SIZE=LONG,-	;
			TYPE=<SPECIAL,DYNAMIC>,-	;
			UNIT=10Ms/Swap	;
;
; Desired process page count for an outswap swap.  This parameter sets the
