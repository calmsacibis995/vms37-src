	PARAMETER	ADDRESS=SCH$GL_WSDEC,-	;
			DEFAULT=35,-	;
			MIN=0,-	;
			NAME=WSDEC,-	;
			SIZE=LONG,-	;
			TYPE=<SYS,DYNAMIC,MAJOR>,-	;
			UNIT=Pages	;
