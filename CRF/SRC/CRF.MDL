;
; CRF USER ARGUMENT LISTS
;

	$STRUCT	CRF

;
; CRF$INSRTREF ARGUMENT LIST
;

	M	1
	F	,L,1				; ARGUMENT COUNT
	F	IR_CTLTBL,L			; CRF CONTROL TABLE ADR
	F	IR_KEYADR,L			; KEY ADDRESS
	F	IR_REFADR,L			; REFERENCE INDICATOR ADR
	F	IR_REFLAG,W			; REFERENCE INDICATOR FLAGS
	F	,B,2				; UNUSED
	F	IR_DEFIND,B			; DEFINITION INDICATOR
	F	,B,3				; UNUSED
	P	1

;
; CRF$INSRTKEY ARGUMENT LIST
;

	F	,L,1				; ARGUMENT COUNT
	F	IK_CTLTBL,L			; CRF CONTROL TABLE ADR
	F	IK_KEYADR,L			; ADDRESS OF KEY
	F	IK_VALADR,L			; ADDRESS OF VALUE
	F	IK_VALFLG,W			; VALUE FLAGS
	F	,B,2				; UNUSED
	P	1

;
; CRF$OUT ARGUMENT LIST
;

	F	,L,1				; ARGUMENT COUNT
	F	OU_CTLTBL,L			; CRF CONTROL TABLE ADR
	F	OU_LINWID,L			; OUTPUT LINE WIDTH
	F	OU_PAG1,B			; # LINES/PAGE 1
	F	,B,3				; UNUSED
	F	OU_SUCPAG,B			; # LINES/SUCCESSIVE PAGES
	F	,B,3				; UNUSED
	F	OU_PRTIND,B			; REF PRINT INDICATOR
	F	,B,3				; UNUSED
	F	OU_SAVIND,B			; SAVE INDICATOR
	P	1

;
; CRF CONTROL TABLE OFFSETS
;


	F	HASHT,L				; HASH TABLE ADDRESS
	F	LASTKEY,L			; LAST KEY LOOKED AT ADDRESS
	F	KEYTYPE,B			; TYPE OF KEY IN TABLE
	V	<
		BINARY				; 1 = KEY IS BINARY 0 = ASCII
		>
	F	FLAGS,B				; MISC. FLAGS FOR FUTURE EXPANSION
	F	ERRSTS,W			; SECONDARY ERROR STATUS CODE
	F	ENTRIES,L			; NUMBER OF ENTRIES IN CROSS REFERENCE
	F	DYNMEM,L,2			; LISTHEAD OF DYNAMIC MEMORY
	F	MAXBLK,L			; SIZE (IN BYTES) OF BLOCK TO ALLOCATE
	F	MEMEXP,L			; SIZE (IN PAGES) OF BLOCK TO ALLOCATE
	F	,L,8				; SPARES
	F	ERROR,L				; ERROR ROUTINE ADDRESS
	F	OUTPUT,L			; OUTPUT ROUTINE ADDRESS
	F	K1FMTBL,L			; KEY1 FIELD FORMAT TBL ADR
	F	K2FMTBL,L			; KEY2 FIELD FORMAT TBL ADR
	F	V1FMTBL,L			; VAL1 FIELD FORMAT TBL ADR
	F	V2FMTBL,L			; VAL2 FIELD FORMAT TBL ADR
	F	R1FMTBL,L			; REF1 FIELD FORMAT TBL ADR
	F	R2FMTBL,L			; REF2 FIELD FORMAT TBL ADR
	E
