$ SET RMS /BUFFER=1/DISK/BLOCK=4
$ DEFINE/USER SYS$OUTPUT NL:
$ PATCH [SYSEXE]MP /OUTPUT=[SYSEXE]MP /JOURNAL=[SYSEXE]MP
DEFINE ECO1_INT = 0
SET ECO 1
DEF ECO1_INT=^X00000D90
RE /I
^X00000F8F
'CMPB R2,R3'
'BEQL ^X00000F99'
'MOVB R2,R3'
'BRB ^X00000F6A'
EXIT
'ADAWI #^X00000000,(R4)'
'CMPL R1,B^^X00000010(R4)'
'BNEQ  ECO1_INT+1DA'
EXI
U
EXI
