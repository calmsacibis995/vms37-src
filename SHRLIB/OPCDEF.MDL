;+
; OPERATOR COMMUNICATIONS MESSAGE TYPES
;-
	$STRUCT	OPC
 
	F	MS_TYPE,B		; MESSAGE TYPE
	F	MS_TARGET,B,1		; MESSAGE TARGET
	S	MS_ENAB,0,B		; MESSAGE ENABLES
	F	MS_STATUS,W		; MESSAGE STATUS
	F	MS_RPLYID,L		; REPLY ID
	S	MS_MASK,0,L		; MESSAGE MASK
	S	MS_RQSTID,0,L		; REQUEST ID
	F	MS_TEXT,L		; MESSAGE TEXT
	S	MS_OUNIT,0,W		; OPERATOR UNIT NUMBER
	S	MS_ONAME,2,T		; OPERATOR NAME
	F	,W,7			; SPACE FOR NAME
	F	MS_OTEXT,L,32		; OPERATOR TEXT
	F	MS_MAXSZ,L		; MESSAGE MAX SIZE
 
	C<OPC,$_,1,1			; OPERATOR MESSAGE TYPES
		RQ_TERME		; ENABLE TERMINAL
		RQ_LOGI			; INITIALIZE THE LOG
		RQ_RQST			; OPERATOR REQUEST
		RQ_REPLY		; OPERATOR REPLY
		RQ_CANCEL		; CANCEL REQUEST
		RQ_STATUS		; REQUEST OPERATOR STATUS
		>
	V<M				; OPERATOR NAMES
		NM_CENTRL,,,M		; 
		NM_PRINT,,,M
		NM_TAPES,,,M
		NM_DISKS,,,M
		NM_DEVICE,,,M
		NM_CARDS,,,M
		NM_NTWORK,,,M
		,5
		NM_OPER1,,,M
		NM_OPER2,,,M
		NM_OPER3,,,M
		NM_OPER4,,,M
		NM_OPER5,,,M
		NM_OPER6,,,M
		NM_OPER7,,,M
		NM_OPER8,,,M
		NM_OPER9,,,M
		NM_OPER10,,,M
		NM_OPER11,,,M
		NM_OPER12,,,M
		>
	E
