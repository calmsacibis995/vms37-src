$ !
$ ! Move file to SYSLIB
$ !
$ SET DEFAULT 'FRONT'SYSLIB]
$ DELETE 'TARGET'DCLTABLES.EXE;*,DELTA.EXE;*,RSXSHR.EXE;*,RSXUSR.EXE;*
