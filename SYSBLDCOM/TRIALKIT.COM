$ SET DEFAULT 'FRONT'SYSEXE]
$ COPY/CONTIG/TRUNC 'EXE'VMB.EXE 'TARGET'
$ ASSIGN/USER 'TARGET''FRONT'SYSEXE] TARGET_DISK
$ MCR WRITEBOOT
TARGET_DISK:VMB.EXE
