$ COPY 'INDEV'[SYSUPD]DXCOPY.COM	'TARGDEV'[SYSUPD]
$ COPY 'INDEV'[SYSUPD]SETDEFBOO.COM	'TARGDEV'[SYSUPD]
$ COPY 'INDEV'[SYSUPD]SWAPFILES.COM	'TARGDEV'[SYSUPD]
