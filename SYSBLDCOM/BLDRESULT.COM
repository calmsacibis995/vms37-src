$ COPY 'INDEV'[SYSUPD]VMS300.COM	'TARGDEV'[SYSUPD]
$ COPY 'INDEV'[SYSUPD]VMSOPT.COM	'TARGDEV'[SYSUPD]
$ COPY 'INDEV'[SYSEXE]SHUTDOWN.COM	'TARGDEV'[SYSEXE]
