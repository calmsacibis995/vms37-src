;
; LAYOUT OF THE FILE IDENTIFICATION BLOCK (FIB)
;
	$STRUCT	FIB
	F ACCTL			; ACCESS CONTROL BITS
	V<M
		NOWRITE		; NO OTHER WRITERS
		DLOCK		; ENABLE DEACCESS LOCK
		BLK_LOCK	; ENABLE RMS-11 BLOCK LOCKING
		,1		; UNUSED
		SPOOL		; SPOOL FILE ON CLOSE
		WRITECK		; ENABLE WRITE CHECK
		SEQONLY		; SEQUENTIAL ONLY ACCESS
		,1		; SPARE
		WRITE		; WRITE ACCESS
		READCK		; ENABLE READ CHECK
		NOREAD		; NO OTHER READERS
		NOTRUNC		; FILE MAY NOT BE TRUNCATED
		,4		; SPARE
				; THE HIGH 8 BITS CANNOT BE COPIED
				; INTO THE ACCESS MODE WORD IN THE WINDOW
		EXECUTE		; ACCESS FOR EXECUTE (USE EXECUTE PROTECTION)
		PRSRV_ATR	; PRESERVE ORIGINAL ATTRIBUTES OF FILE
		RMSLOCK		; OPEN WITH RMS RECORD LOCKING
		WRITETHRU	; FORCE CACHE WRITE-THROUGH ON OPERATION
		NOLOCK		; OVERRIDE ACCESS INTERLOCKS
		NORECORD	; DO NOT RECORD FILE ACCESS
		,2		; SPARE
	>
	V<M			; OVERLAPPING BITS USED FOR MAGTAPE
		,3
		REWIND		; REWIND TAPE
		CURPOS		; CREATE AT CURRENT TAPE POSITION
		,1
		UPDATE		; UPDATE MODE (POSITION TO START OF FILE)
	>
	    S WSIZE,3,B,,S	; WINDOW SIZE
	F FID,W,3		; FILE ID
	    S FID_NUM,,W	; FILE NUMBER
	    S FID_SEQ,,W	; FILE SEQUENCE NUMBER
	    S FID_RVN,,W	; RELATIVE VOLUME NUMBER
	    S FID_RVN,4,B	; SHORT FORM RVN
	    S FID_NMX,,B	; EXTENDED FILE NUMBER
	L ACCDATA		; ABOVE DATA NECESSARY FOR ACCESS
	F DID,W,3		; DIRECTORY ID
	    S DID_NUM,,W	; FILE NUMBER
	    S DID_SEQ,,W	; FILE SEQUENCE NUMBER
	    S DID_RVN,,W	; RELATIVE VOLUME NUMBER
	    S DID_RVN,4,B	; SHORT FORM RVN
	    S DID_NMX,,B	; EXTENDED FILE NUMBER
	F WCC			; WILD CARD CONTEXT
	F NMCTL,W		; NAME CONTROL BITS
	V<M
		,3
		ALLVER		; MATCH ALL VERSIONS
		ALLTYP		; MATCH ALL TYPES
		ALLNAM		; MATCH ALL NAMES
		,2
		WILD		; WILD CARDS IN FILE NAME
		NEWVER		; MAXIMIZE VERSION NUMBER
		SUPERSEDE	; SUPERSEDE EXISTING FILE
		FINDFID		; SEARCH FOR FILE ID
		,2
		LOWVER		; LOWER VERSION OF FILE EXISTS
		HIGHVER		; HIGHER VERSION OF FILE EXISTS
	>
	L DIRDATA		; ABOVE DATA NECESSARY FOR DIRECTORY OPS
	M 1			; MARK PARAMETER VARIATION
	F EXCTL,W		; EXTEND CONTROL
	V<M
		ALCON		; ALLOCATE CONTIGUOUS
		ALCONB		; CONTIGUOUS BEST EFFORT
		FILCON		; MARK FILE CONTIGUOUS
		ALDEF		; ALLOCATE DEFAULT AMOUNT
		ALLOCATR	; PLACEMENT DATA PRESENT IN ATTRIBUTE LIST
		,2
		EXTEND		; ENABLE EXTENSION
		TRUNC		; ENABLE TRUNCATION
		NOHDREXT	; INHIBIT EXTENSION HEADERS
		MARKBAD		; MARK BLOCKS BAD
		,4
		NOCHARGE	; DON'T CHARGE DISKQUOTA
	>
	M 2			; MARK PARAMETER VARIATION
	F EXSZ			; EXTEND SIZE
	F EXVBN			; EXTENSION VBN
	L EXTDATA		; ABOVE NECESSARY FOR BASIC FILE EXTENSION
	F ALOPTS,B		; ALLOCATION OPTIONS
	V<M
		EXACT		; EXACT PLACEMENT REQUIRED
		ONCYL		; PUT ALLOCATION ON ONE CYLINDER
	>
	F ALALIGN,B		; ALLOCATION ALIGNMENT
		C CYL,1		; CYLINDER ADDRESS SPECIFIED
		C LBN,2		; LBN SPECIFIED
		C VBN,3		; PROXIMATE VBN SPECIFIED
		C RFI,4		; RELATED FILE ID SPECIFIED
	F ALLOC,W,5		; ALLOCATION LOCATION
	    S LOC_FID,,W,3	; RELATED FILE ID
	    S LOC_NUM,0,W	; RELATED FILE NUMBER
	    S LOC_SEQ,,W	; FILE SEQUENCE NUMBER
	    S LOC_RVN,,W	; RELATED RVN
	    S LOC_RVN,4,B	; SHORT FORM RVN
	    S LOC_NMX,,B	; EXTENDED FILE NUMBER
	    S LOC_ADDR		; LOCATION ADDRESS (VBN, LBN, CYL)
	L ALCDATA		; ABOVE DATA NECESSARY FOR PLACEMENT
	F VERLIMIT,W		; DIRECTORY ENTRY VERSION LIMIT
	F ,W			; SPARE
	L LENGTH

	P 1			; START OF ACP CONTROL PARAMETERS
	F CNTRLFUNC,W		; ACP CONTROL FUNCTION
; DEFINE ACP CONTROL FUNCTION CODES
;
	K <FIB,$C_,1,1
		REWINDVOL	; REWIND VOLUME SET
		POSEND		; POSITION TO END OF VOLUME SET
		NEXTVOL		; FORCE NEXT VOLUME
		SPACE		; SPACE MAGNETIC TAPE
		ILLEGAL		;
		REWINDFIL	; REWIND FILE
		LOCK_VOL	; LOCK VOLUME AGAINST ALLOCATION
		UNLK_VOL	; UNLOCK VOLUME
				; QUOTA FILE OPERATIONS
		ENA_QUOTA	; ENABLE QUOTA FILE
		DSA_QUOTA	; DISABLE QUOTA FILE
		ADD_QUOTA	; ADD QUOTA FILE ENTRY
		EXA_QUOTA	; EXAMINE QUOTA FILE ENTRY
		MOD_QUOTA	; MODIFY QUOTA FILE ENTRY
		REM_QUOTA	; REMOVE QUOTA FILE ENTRY
		USEREOT		; ENABLE USER END OF TAPE HANDLING
		REMAP		; REMAP FILE WINDOW
	  >
	F CNTRLVAL		; ACP CONTROL FUNCTION VALUE PARAMETER
	V <M			; CONTROL BITS USED IN QUOTA FILE OPERATIONS
		ALL_MEM		; MATCH ALL MEMBER NUMBERS
		ALL_GRP		; MATCH ALL GROUP NUMBERS
		MOD_USE		; MODIFY USAGE DATA
		MOD_PERM	; MODIFY PERMANENT QUOTA
		MOD_OVER	; MODIFY OVERDRAFT LIMIT
	>


	L MTALEN		; LENGTH OF MTAACP DATA

	E

;
; STRUCTURE OF A FILE ID (FID)
;

	$STRUCT	FID

	F NUM,W			; FILE NUMBER
	F SEQ,W			; FILE SEQUENCE NUMBER
	F RVN,W			; RELATIVE VOLUME NUMBER
	S RVN,,B		; BYTE FORM OF RVN
	S NMX,,B		; FILE NUMBER EXTENSION
	L LENGTH
				; FILE ID'S OF THE RESERVED FILES
	C<FID,$C_,1,1
	INDEXF			; INDEX FILE
	BITMAP			; STORAGE MAP FILE
	BADBLK			; BAD BLOCK FILE
	MFD			; MASTER FILE DIRECTORY
	CORIMG			; CORE IMAGE FILE
	VOLSET			; VOLUME SET LIST FILE
	CONTIN			; STANRARD CONTINUATION FILE
	BACKUP			; BACKUP LOG FILE
	BADLOG			; BAD BLOCK LOG FILE
	FREFIL			; FREE SPACE FILE
	>

	E

;+
; IMAGE ACTIVATION CONTROL FLAGS
;-

	$STRUCT	IAC
	V<M
		NOACT			;DO NOT ACTIVATE THE IMAGE (FOR INSTALL)
		WRITABLE		;MAKE IMAGE FILE WRITABLE
		SHAREABLE		;ACT. SHAREABLE IMAGE FOR EXECUTABLE IMG
		PRIVILEGE		;ACT. SHARE. IMG FOR PRIV EXECUTABLE IMG
		MERGE			;ACT 2ND EXECUTABLE IMG INTO ADR SPACE
		EXPREG			;MAP IMAGE INTO NEXT FREE VA SPACE
		P1MERGE			;P1 MERGED ACTIVATION (LEGAL INPUT FLAG)
		,1			;SPARE
		LASTCLU			;LAST CLUSTER FLAG
		LIM			;LINKABLE IMAGE
		RETRY			;RETRY IMAGE ACTIVATION
		NOCMKRNL		;SHUT OFF CMKRNL,CMEXEC-SYSVER DIFF
		SEQDEVLOD		;LOADING FROM SEQUENTIAL DEVICE (NET)
		XLINKER			;CROSS LINKER FORMAT
		KP_RESHDR		;MAKE IMAGE HEADER RESIDENT
		IS_RESHDR		;IMAGE HEADER IS RESIDENT
		NOTSHARED		;DO NOT SET IS_SHARED IN KFI ENTRY
		GBLCLUSTR		;CURRENTLY PROCESSING GBL ISD CLUSTER
		SHMIDENT		;SHARED MEMORY IDENT USED FOR GBL SEC
		NOCOPY			;NO PRIVATE COPY OF SECTION IN EXEC IMG
		P1MERG_P0		;P1 MERGED ACTIVATION WITH P0 ADDRESS
					; RANGE (INTERNAL FLAG ONLY)
		SETVECTOR		;SIGNAL ATLERNATE ENTRY TO SET VECTORS
		>

	C<
		LARGEST,6		;LARGEST FLAG CALLER MAY SPECIFY
		>
	E

;+
; I/O FUNCTION CODE DEFINITIONS
;-
 
	$STRUCT	IO
 
;
; *** START PHYSICAL I/O FUNCTION CODES ***
;
 
	C<,$_
		NOP,0			;NO OPERATION
		UNLOAD,1		;UNLOAD DRIVE
		LOADMCODE,1		;LOAD MICROCODE
		SEEK,2			;SEEK CYLINDER
		SPACEFILE,2		;SPACE FILES
		STARTMPROC,2		;START MICROPROCESSOR
		RECAL,3			;RECALIBRATE DRIVE
		STOP,3			;STOP
		DRVCLR,4		;DRIVE CLEAR
		INITIALIZE,4		;INITIALIZE
		RELEASE,5		;RELEASE PORT
		SETCLOCKP,5		;SET CLOCK (PHYSICAL)
		OFFSET,6		;OFFSET READ HEADS
		ERASETAPE,6		;ERASE TAPE
		STARTDATAP,6		;START DATA TRANSFER (PHYSICAL)
		RETCENTER,7		;RETURN TO CENTERLINE
		QSTOP,7			;QUEUE STOP REQUEST
		PACKACK,8		;PACK ACKNOWLEDGE
		SEARCH,9		;SEARCH FOR SECTOR
		SPACERECORD,9		;SPACE RECORDS
		WRITECHECK,10		;WRITE CHECK DATA
		WRITEPBLK,11		;WRITE PHYSICAL BLOCK
		READPBLK,12		;READ PHYSICAL BLOCK
		WRITEHEAD,13		;WRITE HEADER AND DATA
		RDSTATS,13		;READ STATISTICS
		READHEAD,14		;READ HEADER AND DATA
		WRITETRACKD,15		;WRITE TRACK DATA
		READTRACKD,16		;READ TRACK DATA
		AVAILABLE,17		;AVAILABLE (DISK AND TAPE CLASS)
		DSE,21			;DATA SECURITY ERASE (AND REWIND)
		REREADN,22		;REREAD NEXT
		REREADP,23		;REREAD PREVIOUS
		WRITERET,24		;WRITE RETRY
		WRITECHECKH,24		;WRITE CHECK HEADER AND DATA
		READPRESET,25		;READIN PRESET
		STARTSPNDL,25		;START SPINDLE
		SETCHAR,26		;SET CHARACTERISTICS
		SENSECHAR,27		;SENSE TAPE CHARACTERISTICS
		WRITEMARK,28		;WRITE TAPE MARK
		WRTTMKR,29		;WRITE TAPE MARK RETRY
		DIAGNOSE,29		;DIAGNOSE
		FORMAT,30		;FORMAT
		CLEAN,30		;CLEAN TAPE
		PHYSICAL,31		;HIGHEST PHYSICAL I/O FUNCTION CODE
	>
 
;
; *** START LOGICAL I/O FUNCTION CODES ***
;
 
	C<,$_
		WRITELBLK,32		;WRITE LOGICAL BLOCK
		READLBLK,33		;READ LOGICAL BLOCK
		REWINDOFF,34		;REWIND AND SET OFFLINE
		SETMODE,35		;SET MODE
		REWIND,36		;REWIND TAPE
		SKIPFILE,37		;SKIP FILES
		SKIPRECORD,38		;SKIP RECORDS
		SENSEMODE,39		;SENSE TAPE MODE
		WRITEOF,40		;WRITE END OF FILE
		LOGICAL,47		;HIGHEST LOGICAL I/O FUNCTION CODE
	>
 
;
; *** START VIRTUAL I/O FUNCTION CODES
;
 
	C<,$_
		WRITEVBLK,48		;WRITE VIRTUAL BLOCK
		READVBLK,49		;READ VIRTUAL BLOCK
		ACCESS,50		;ACCESS FILE
		CREATE,51		;CREATE FILE
		DEACCESS,52		;DEACCESS FILE
		DELETE,53		;DELETE FILE
		MODIFY,54		;MODIFY FILE
		NETCONTROL,54		;X25 NETWORK CONTROL FUNCTION
		READPROMPT,55		;READ TERMINAL WITH PROMPT
		SETCLOCK,55		;SET CLOCK
		FORCE,55		;JOURNALING BUFFER FLUSH
		ACPCONTROL,56		;MISCELLANEOUS ACP CONTROL
		STARTDATA,56		;START DATA
		MOUNT,57		;MOUNT VOLUME
		TTYREADALL,58		; TERMINAL READ PASSALL
		ENDRU1,58		; JOURNAL END RECOVERY UNIT PHASE 1
		TTYREADPALL,59		; TERM READ W/PROMPT PASSALL
		ENDRU2,59		; JOURNAL END RECOVERY UNIT PHASE 2
		CONINTREAD,60		; Connect to interrupt readonly
		READINIT,60		; JOURNAL READ INITIALIZE
		CONINTWRITE,61		; Connect to interrupt with write
		VIRTUAL,63		;HIGHEST VIRTUAL I/O FUNCTION
	>
 
;
; FUNCTION MODIFIER BIT DEFINITIONS
;
 
	V<M				; MODIFIER BIT DEFINITIONS
		FCODE,6,,M		; FUNCTION CODE FIELD
