		NOFILTR,,,M		;NO FILTER
		DSABLMBX,,,M		;DISABLE MAILBOX
		PURGE,,,M		;PURGE TYPEAHEAD
		TRMNOECHO,,,M		;TERMINATORS ARE NOT ECHOED
		REFRESH,,,M		;Control-R interrupted read
		ESCAPE,,,M		;TERMINATE READ ON ESCAPE SEQUENCE
		EXTEND,,,M		;USED BY ALTERNATE CLASS DRIVERS
	>
 
	V<M				;TERMINAL WRITE FUNCTION MODIFIERS
		,6			; FUNCTION CODE
		CANCTRLO,,,M		;CANCEL CONTROL O
		ENABLMBX,,,M		;ENABLE MAILBOX
		NOFORMAT,,,M		;DO NOT FORMAT OUTPUT
	>
 
	V<M				;TERMINAL SETMODE SUBFUNCTIONS
		,6			; RESERVE FUNCTION CODES
		MAINT,,,M		;ENABLE MAINT SUB MODIFIERS
		CTRLYAST,,,M		;SET CONTROL Y AST
		CTRLCAST,,,M		;SET CONTROL C
		HANGUP,,,M		;SET MODE AND HANG UP LINE
		OUTBAND,,,M		;SET OUT OF BAND AST
		ENABL_ALT,,,M		;ENABLE ALTERNATE PROCESSING
