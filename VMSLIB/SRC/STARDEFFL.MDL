		READATTN,,,M		;REQUEST AST ON WAITING READER
		WRTATTN,,,M		;REQUEST AST ON WAITING WRITER
		SETPROT,,,M		;SET VOLUME PROTECTION
	>
 
	V<M				;TERMINAL READ FUNCTION MODIFIERS
		,6			;FUNCTION CODE
		NOECHO,,,M		;NOECHO
		TIMED,,,M		;TIMED
