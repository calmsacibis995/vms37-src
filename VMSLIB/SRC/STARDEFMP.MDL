		BOT,,,M			; AT BEGINNING OF TAPE
		EOF,,,M			; AT END OF FILE
		EOT,,,M			; AT END OF TAPE
		HWL,,,M			; TAPE IS HARDWARE WRITELOCKED
		LOST,,,M		; TAPE POSITION LOST
	>
 
